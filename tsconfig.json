{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "lib": ["es6", "dom"],
        "module": "commonjs",
        "target": "es5",
        "downlevelIteration": true,
        "jsx": "react",
        "noImplicitAny": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    },
    "include": [
        "./src/**/*"
    ]
}